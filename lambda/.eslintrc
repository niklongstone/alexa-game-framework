{
    "env": {
        "node": true,
    },
    "extends": "airbnb-base",
    "rules": {
        "strict": 0,
        "prefer-destructuring": 0,
        "func-names": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "max-len": 0,
        "radix": 0,
        "no-param-reassign": 0
    }
}
